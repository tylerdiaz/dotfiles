[user]
  name = Tyler J. Diaz
  username = tylerdiaz
  email = me@tylerdiaz.com
[filter "media"]
  required = true
  clean = git media clean %f
  smudge = git media smudge %f
[core]
  autocrlf = input
  attributesfile = /Users/tylerdiaz/.gitattributes
[color]
  ui = true
  diff = auto
[push]
  default = current
[url "https://"]
  insteadOf = git://
[alias]
  gr = grep -n
  s = status
  tree = log --graph --decorate --pretty=oneline --abbrev-commit
[status]
  showUntrackedFiles = all
[credential]
  helper = osxkeychain
[filter "lfs"]
  clean = git lfs clean %f
  smudge = git lfs smudge %f
  required = true
[merge]
  tool = ediff
[mergetool "ediff"]
  cmd = emacs -c -a \"\"  --eval \"(ediff-merge-files-with-ancestor \\\"$LOCAL\\\" \\\"$REMOTE\\\" \\\"$BASE\\\" nil \\\"$MERGED\\\")\"
